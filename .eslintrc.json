{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "off",
    "strict": "off",
    "lines-around-directive": "off",
    "dot-notation": "off",
    "object-curly-spacing": "off",
    "prefer-destructuring": "off",
    "no-await-in-loop": "off",
    "max-len": "off",
    "object-curly-newline": "off",
    "no-param-reassign": "off"
  }
}
